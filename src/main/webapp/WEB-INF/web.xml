<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
<!--  servlet declaration-->
  <servlet>
    <servlet-name>first</servlet-name>
    <servlet-class>com.servlet.FirstServlet</servlet-class>
  </servlet>
<servlet>
  <servlet-name>second</servlet-name>
  <servlet-class>com.servlet.SecondServlet</servlet-class>
</servlet>

  <servlet>
    <servlet-name>third</servlet-name>
    <servlet-class>com.servlet.ThirdServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>registerServlet</servlet-name>
    <servlet-class>com.servlet.submitForm.RegisterServlet</servlet-class>
  </servlet>
<!--  mapping-->
  <servlet-mapping>
    <servlet-name>first</servlet-name>
    <url-pattern>/web</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>second</servlet-name>
    <url-pattern>/web_generic</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>third</servlet-name>
    <url-pattern>/web_http</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>registerServlet</servlet-name>
    <url-pattern>/RegisterServlet</url-pattern>
  </servlet-mapping>

</web-app>
